<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.ncsu.beans">
	<class name="Template" table="templates">
		<id name="temp_id" column="temp_id">
        	<generator class="native"/>
        </id>
        <property name="type_id" column="type_id"/>
        <property name="temp_name" column="temp_name"/>
        <property name="pathname" column="pathname"/>
	</class>
	
</hibernate-mapping>